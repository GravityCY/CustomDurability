{
  "schemaVersion": 1,
  "id": "${mod_id}",
  "name": "${mod_name}",
  "version": "${mod_version}",
  "description": "${mod_description}",
  "authors": ["${mod_author}"],

  "contact": {
    "homepage": "${mod_sources}",
    "sources": "${mod_sources}",
    "issues": "${mod_sources}"
  },
  "license": "${mod_license}",
  "icon": "assets/customdurability/icon.png",

  "environment": "*",
  "entrypoints": {
    "preLaunch": [
      "me.gravityio.customdurability.CustomDurabilityMod"
    ],
    "main": [
      "me.gravityio.customdurability.CustomDurabilityMod"
    ],
    "client":[
      "me.gravityio.customdurability.client.CustomDurabilityClientMod"
    ]
  },
  "mixins": [
    "customdurability.mixins.json"
  ],
  "depends": {
    "fabricloader": "*",
    "minecraft": "1.20.x",
    "java": ">=17"
  }
}